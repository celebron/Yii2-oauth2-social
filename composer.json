{
    "name": "celebron/yii2-social-auth",
    "type": "yii2-extension",
    "keywords": ["yii2", "extension","celebron"],
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "Celebron\\socialSource\\": "src/",
            "Celebron\\socials\\": "socials/"
        }
    },
    "authors": [
        {
            "name": "Aleksey Shatalin (celebron)",
            "email": "celebron.ru@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "github",
            "url": "https://github.com/celebron/yii2-common.git"
        }
    ],
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^10.2"

    },
    "require": {
        "yiisoft/yii2-httpclient": "^2.0",
        "yiisoft/yii2": "^2.0",
        "yiisoft/http": "^1.2",
        "php": "^8.2",
        "celebron/yii2-celebron-common": "^1.1",
        "yiisoft/yii2-bootstrap5": "^2.0"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
