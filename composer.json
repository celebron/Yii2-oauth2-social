{
    "name": "celebron/yii2-social-auth",
    "type": "yii2-extension",
    "autoload": {
        "psr-4": {
            "Celebron\\social\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Celebron\\social\\tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "celebron",
            "email": "celebron.ru@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "yiisoft/yii2-apidoc": "*",
        "codeception/codeception": "^4.2",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^1.0.0"
    },
    "require": {
        "yiisoft/yii2-httpclient": "^2.0",
        "yiisoft/yii2": "^2.0",
        "yiisoft/http": "^1.2",
        "php": "^8.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3.x-dev"
        }
    }
}