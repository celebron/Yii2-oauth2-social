{
    "name": "celebron/yii2-social-auth",
    "type": "yii2-extension",
    "keywords": ["yii2", "extension"],
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "Celebron\\social\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Celebron\\social\\tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Aleksey Shatalin (celebron)",
            "email": "celebron.ru@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "yiisoft/yii2-apidoc": "*",
        "codeception/codeception": "^4.2",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^1.0.0"
    },
    "require": {
        "yiisoft/yii2-httpclient": "^2.0",
        "yiisoft/yii2": "^2.0",
        "yiisoft/http": "^1.2",
        "bower-asset/bootstrap": "^5.1.0",
        "php": "^8.1"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    }
}
